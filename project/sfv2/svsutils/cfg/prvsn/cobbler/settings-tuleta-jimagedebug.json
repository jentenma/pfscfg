#START-svsutils-variables
# These will get appended to /etc/cobbler/settings. It will require a cobbler
# restart to read them.   systemctl restart cobblerd.service
# --clobber should get rid if these and --init will append them and restart.

# path where svsutils in installed
prvsn_path: "/opt/ibm/appliance/platform/svsutils/scripts"

# What directory gets exported by the server
prvsn_export_dir: "/home/backup"

# Provisioner files suitable for curl are placed here.
# See httpd.conf DocumentRoot 
# Example: prvsn_webdir: "/var/www/images/prvsn-files"
# cat /etc/httpd/conf/httpd.conf | grep ^DocumentRoot | sed 's/\"//g" | awk '{print $2}'

prvsn_webdir: "DOCUMENTROOT_GOES_HERE/prvsn-files"

#END-svsutils-variables
